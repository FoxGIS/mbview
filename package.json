{
  "name": "mbview",
  "version": "1.15.2",
  "description": "Watch mbtiles in your localhost",
  "bin": {
    "mbview": "./cli.js"
  },
  "main": "server.js",
  "scripts": {
    "lint": "eslint *.js test/",
    "pretest": "npm run lint",
    "test": "tape test/*.test.js",
    "start": "node cli.js",
    "pkg": "pkg . --out-path=dist/"
  },
  "keywords": [
    "localhost",
    "mapbox",
    "mbtiles"
  ],
  "author": "Rodolfo Wilhelmy <rwilhelmy@gmail.com> (http://wilhel.me)",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^2.13.1",
    "pkg": "^4.3.0",
    "supertest": "^1.2.0",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "d3-queue": "^2.0.3",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "mbtiles": "^0.9.0",
    "minimist": "^1.2.0",
    "object-assign": "^4.1.0",
    "open": "0.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mapbox/mbview.git"
  },
  "bugs": {
    "url": "https://github.com/mapbox/mbview/issues"
  },
  "homepage": "https://github.com/mapbox/mbview#readme",
  "pkg": {
    "targets": [
      "node6-macos-x64",
      "node6-linux-x64",
      "node6-win-x64"
    ],
    "assets": [
      "node_modules/mbtiles/lib/schema.sql",
      "views/*",
      "public/*"
    ]
  }
}
